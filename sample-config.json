{
  "baseUrl": "http://demo.ribbl.com/install/10979_646bf/",
  "format": "png",
  "outDir": "output",
  "login": {
    "url": "login",
    "inject": "location.href = $('.login-link:first').attr('href');"
  },
  "screens": {
    "desktop": {
      "width": 1280,
      "height": 720,
      "ratio": 1
    },
    "nexus5": {
      "width": 360,
      "height": 640,
      "ratio": 2
    }
  },
  "shots": {
    "default-shot": {
      "url": "startseite",
      "screen": "desktop"
    },
    "clipped-shot": {
      "url": "startseite",
      "screen": "desktop",
      "region": {
        "left": 100,
        "top": 100,
        "width": 500,
        "height": 20
      }
    },
    "dom-element": {
      "url": "startseite",
      "screen": "desktop",
      "element": ".streamEditorWidget"
    },
    "inject-code": {
      "url": "startseite",
      "screen": "desktop",
      "inject": "$('.microblogMicroblogStreamEditor textarea').focus();"
    }
  }
}
